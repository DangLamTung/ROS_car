<!-- 
  Simulate a differential drive robot with the teb_local_planner in stage:
  - stage
  - map_server
  - move_base
  - static map
  - amcl
  - rviz view
 -->
<launch>

        <!--  ************** Global Parameters ***************  -->
        <!-- <param name="/use_sim_time" value="true"/> -->
    <!-- <arg name="no_static_map" value="true"/> -->
        <!--  ************** Unity Simulator ***************  -->
	<node pkg="thesis_car" type="connect_simulation.py" name="connect_simulation" output="screen"></node>
    <node pkg="thesis_car" type="osm_planner.py" name="global_planner" output="screen"></node>
    <!-- <node pkg="tf" type="static_transform_publisher" name="map" args="0 0 0 0 0 0 /base_link /map 100" /> -->
    <!--  ************** Navigation ***************  -->
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

	    <!-- <remap from="/move_base/TebLocalPlannerROS/global_plan" to="/move_base/OSMPlanner/plan"/> -->
  	  	<rosparam file="$(find teb_local_planner_tutorials)/cfg/carlike/costmap_common_params.yaml" command="load" ns="global_costmap" />
  	 	<rosparam file="$(find teb_local_planner_tutorials)/cfg/carlike/costmap_common_params.yaml" command="load" ns="local_costmap" />
  		<rosparam file="$(find teb_local_planner_tutorials)/cfg/carlike/local_costmap_params.yaml" command="load" />
  		<rosparam file="$(find teb_local_planner_tutorials)/cfg/carlike/global_costmap_params.yaml" command="load" />
  		<rosparam file="$(find teb_local_planner_tutorials)/cfg/carlike/teb_local_planner_params.yaml" command="load" />

		<param name="base_global_planner" value="global_planner/GlobalPlanner" />
		<param name="planner_frequency" value="1" />
		<param name="planner_patience" value="5.0" />

		<param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
		<param name="controller_frequency" value="5.0" />
		<param name="controller_patience" value="15.0" /> -->

        <param name="clearing_rotation_allowed" value="false" /> 
	</node>



	<!--  ****** Maps *****  -->
	<node name="map_server" pkg="map_server" type="map_server" args="$(find teb_local_planner_tutorials)/maps/blank.yaml" output="screen">
                <param name="frame_id" value="map"/>
	</node>
	 <!-- <include file="$(find thesis_car)/launch/car_robot_localization.launch" /> -->

	<!-- <node pkg="amcl" type="amcl" name="amcl" output="screen">
		<rosparam file="$(find teb_local_planner_tutorials)/cfg/amcl_params.yaml" command="load" />

	        <param name="initial_pose_x"            value="2"/>
		<param name="initial_pose_y"            value="2"/>
		<param name="initial_pose_a"            value="0"/>
	</node> -->

	  <!--  **************** Visualisation ****************  -->
    <!-- Start rviz with a pre-configured AerialMap instance. It will use the fake GPS fix from above. -->
    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find rviz_satellite)/launch/demo.rviz"/> -->
     <!--  **************** Visualisation ****************  -->
	  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find thesis_car)/cfg/rviz_navigation.rviz"/>
    <!-- Static fake TF transform -->
	 <node pkg="tf" type="static_transform_publisher" name="map" args="0 0 0 0 0  -0.707  -0.707 map utm 100" />
    <node pkg="tf" type="static_transform_publisher" name="camera_static" args="20 0 0 0 0  -0.707  -0.707 base_link base_link_cam 100" />

</launch>
